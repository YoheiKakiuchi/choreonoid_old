isNewProjectTemplate: true

optionalPlugins: [ Python ]

views: 
  - 
    id: 1
    plugin: Base
    class: ItemPropertyView
    mounted: true
  - 
    id: 2
    plugin: Base
    class: ItemTreeView
    mounted: true
  - 
    id: 3
    plugin: Base
    class: LocationView
    mounted: true
  - 
    id: 4
    plugin: Base
    class: MessageView
    mounted: true
  - 
    id: 5
    plugin: Base
    class: SceneView
    mounted: true
    state: 
      lightingMode: full
  - 
    id: 7
    plugin: Body
    class: JointDisplacementView
    mounted: true
    state: 
      show_joint_ids: true
      show_joint_names: false
  - 
    id: 8
    plugin: Body
    class: LinkDeviceListView
    mounted: true
  - 
    id: 9
    plugin: Body
    class: LinkPositionView
    mounted: true
  - 
    id: 10
    plugin: Body
    class: LinkPropertyView
    mounted: true
  - 
    id: 11
    plugin: Python
    class: PythonConsoleView
    mounted: true

viewAreas: 
  - 
    type: embedded
    tabs: true
    contents: 
      type: splitter
      orientation: horizontal
      sizes: [ 400, 100 ]
      children: 
        - 
          type: splitter
          orientation: horizontal
          sizes: [ 140, 400 ]
          children: 
            - 
              type: splitter
              orientation: vertical
              sizes: [ 100, 100 ]
              children: 
                - 
                  type: pane
                  views: [ 2 ]
                - 
                  type: pane
                  views: [ 1, 10 ]
                  current: 1
            - 
              type: splitter
              orientation: vertical
              sizes: [ 300, 100 ]
              children: 
                - 
                  type: pane
                  views: [ 5 ]
                - 
                  type: pane
                  views: [ 4, 11 ]
                  current: 1
        - 
          type: splitter
          orientation: vertical
          sizes: [ 1, 600 ]
          children: 
            - 
              type: pane
              views: [ 3 ]
            - 
              type: splitter
              orientation: vertical
              sizes: [ 1, 400 ]
              children: 
                - 
                  type: pane
                  views: [ 9 ]
                - 
                  type: pane
                  views: [ 8, 7 ]
                  current: 7
layoutOfToolBars: 
  rows: 
    - 
      - { name: "FileBar", x: 0, priority: 0 }
      - { name: "ScriptBar", x: 0, priority: 1 }
      - { name: "SimulationBar", x: 0, priority: 2 }
      - { name: "TimeBar", x: 0, priority: 0 }
    - 
      - { name: "BodyBar", x: 0, priority: 0 }
      - { name: "KinematicsBar", x: 0, priority: 1 }
      - { name: "SceneBar", x: 0, priority: 2 }
      - { name: "GraphBar", x: 0, priority: 3 }
      - { name: "CaptureBar", x: 0, priority: 4 }
